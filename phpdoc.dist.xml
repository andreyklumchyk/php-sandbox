<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="data/xsd/phpdoc.xsd"
>
    <title>Chat PHP Documentation</title>
    <paths>
        <output>_build/artifacts/rootfs/docs</output>
        <cache>.cache/phpdoc/</cache>
    </paths>
    <version number="3.0.0">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
            <default-package-name>Chat</default-package-name>
        </api>
    </version>
</phpdocumentor>
